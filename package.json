{
  "name": "covies-api",
  "version": "1.0.0",
  "description": "Simple movie API server.",
  "scripts": {
    "dev": "export NODE_ENV=development&& nodemon --exec babel-node ./src/server.js --delay 1",
    "introspect:local": "rover subgraph introspect http://localhost:3001/graphql > ./src/graphql/generated/schema.graphql",
    "introspect:dev": "rover subgraph introspect https://covies-development.herokuapp.com/graphql > ./src/graphql/generated/schema.graphql",
    "publish:local": "rover subgraph publish cateway-production@current --name covies --routing-url http://localhost:3001/graphql --schema ./src/graphql/generated/schema.graphql",
    "publish:dev": "rover subgraph publish cateway-production@current --name covies --routing-url https://covies-development.herokuapp.com/graphql --schema ./src/graphql/generated/schema.graphql",
    "build": "babel src --out-dir build",
    "start": "node build/server"
  },
  "keywords": [
    "movies",
    "api",
    "graphql"
  ],
  "author": "Codeliner96 <codeliner96@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-regenerator": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.8"
  },
  "dependencies": {
    "@apollo/subgraph": "^0.2.0",
    "@babel/cli": "^7.17.0",
    "@babel/runtime": "^7.17.2",
    "@graphql-tools/load-files": "^6.5.3",
    "@graphql-tools/merge": "^8.2.1",
    "apollo-server-core": "^3.6.1",
    "apollo-server-express": "^3.6.2",
    "axios": "^0.24.0",
    "dotenv": "^13.0.1",
    "express": "^4.17.2",
    "graphql": "^16.2.0",
    "graphql-upload": "^13.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15"
  }
}
